{
  "name": "training",
  "version": "1.0.0",
  "main": ".eleventy.js",
  "scripts": {
    "build": "npx @11ty/eleventy --input=. --output=_site",
    "build:local": "npx @11ty/eleventy --input=. --output=_site_local",
    "build:css": "./node_modules/.bin/tailwindcss -c tailwind.config.js -i input.css -o _site_local/styles.css --minify",
    "start:local": "npm run build:local && npm run build:css && open _site_local/index.html"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  },
  "keywords": [],
